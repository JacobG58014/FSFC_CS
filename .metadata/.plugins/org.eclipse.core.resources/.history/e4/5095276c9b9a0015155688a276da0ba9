package Main;

import java.io.*;

public class TryCatch {
	String test;
	public static void FileReaderUtil(){

		File Jacob = new File("Jacob.txt");
		
			Reader fw = null;
			try {
				fw = new FileReader(Jacob);
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			/*
			try {
				fw =
				fw.write("HELLO");
			} catch (IOException e) {
					System.out.println("Fail");
				e.printStackTrace();
			}
			try {
				fw.write("FileW");
			} catch (IOException e) {
				System.out.println("NO");				
				e.printStackTrace();
			}
			*/
			BufferedReader bw = new BufferedReader(fw);
			try {
				test=bw.readLine();
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			//FileReader ReadJacob = new FileReader(Jacob);
		}
	

		
	
	public static void main(String args[]){
		FileReaderUtil();
		System.out.println(test);
	}
	public static void write(BufferedWriter bw){
		try {
			bw.write("Buffered");
		} catch (IOException e) {
			e.printStackTrace();
			System.out.println("NOPE");
		}	
	}
}
