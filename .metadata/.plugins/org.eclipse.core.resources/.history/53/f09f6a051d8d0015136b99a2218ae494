package Application;

public class BubbleSort {
	public static void main (String args []){
		
		
		
		int[] x = new int[10];

		for (int i=0; i < x.length; i++ ){
			x[i] = (int) (1+ Math.random() * 10);
		}
		printArray(x);
		}
		
		public static void printArray(int[] x){
		//Random array 1
		for (int i = 0; i < x.length; i++){
			System.out.print(x[i] + " ");
		}
		System.out.println("");
		int tempVal=0;
		//bubble sort
		for(int i=0;i<x.length-1;++i){
		
		for(int j=0;j<x.length-1;++j){
		if(x[j]>x[j+1]){
		tempVal = x[j];
		x[j]=x[j+1];
		x[j+1]=tempVal;
		}
		}
		}
		
		
		
		//Random array 2
		for(int i=0; i<x.length; ++i){
		System.out.print((x[i] + " "));
		}
		System.out.println("");
		int[] y = new int[10];

		for (int i=0; i < y.length; i++ ){
			y[i] = (int) (1+ Math.random() * 10);
		}System.out.println("");
		printArray2(y);
		//Insertion sort
		int tempVal2 = 0;
		int currentPos=1;
		
		for(currentPos=1; currentPos<y.length;++currentPos){
			while (currentPos>0 && y[currentPos]<y [currentPos - 1]){
			tempVal2=y[currentPos];
			y[currentPos]=y[currentPos-1];
			y[currentPos-1]=tempVal;
			--currentPos;
			
			}
		}printArray2(y);
			}
		
		public static void printArray2(int[] y){
		
		for (int i = 0; i < y.length; i++){
			System.out.print(y[i] + " ");
		}
		System.out.println("");
		}
		
}
	
